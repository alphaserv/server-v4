# IRC Bot Vars
global irc_network "irc.gamesurge.net" # IRC Network to connect to
global irc_network_port 6667 # Port the irc bot will connect to the irc network on
global irc_adminpw "changemenow" # Not used.
global irc_debug 0 # Enables debugging info in server log
global irc_channel "#qs-server" # The channel where the game chat will be displayed
global irc_bot_command_name "hop" # The name the bot will respond to commands with
global irc_bot_name "{HOP}HouseofPain" # The name that is displayed in the channel
global irc_network_username "hop" #This can stay hop
